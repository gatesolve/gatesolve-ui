  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "tsc --noemit && react-scripts test --testPathIgnorePatterns \"src/__tests__/\"",
    "test-e2e": "tsc --noemit && react-scripts test --testPathPattern \"src/__tests__/\"",
    "test-all": "tsc --noemit && react-scripts test",
    "eject": "react-scripts eject",
    "eslint-check": "eslint --print-config src/App.tsx | eslint-config-prettier-check",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "prettier-check": "prettier --check .eslintrc.js package.json tsconfig.json \"{src,public,.github}/**/*{.js,.jsx,.ts,.tsx,.json,.css,.yaml}\"",
    "prettier": "prettier --write .eslintrc.js package.json tsconfig.json \"{src,public,.github}/**/*{.js,.jsx,.ts,.tsx,.json,.css,.yaml}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
